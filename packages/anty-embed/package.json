{
  "name": "@antfly/anty-embed",
  "version": "0.2.0",
  "type": "module",
  "description": "Embeddable Anty character animation for web applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles/anty-embed.css"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "gsap": "^3.14.0",
    "@gsap/react": "^2.1.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.400.0",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "rollup": "^4.28.0",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.7.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/searchaf/anty"
  },
  "keywords": [
    "anty",
    "animation",
    "character",
    "search",
    "embed",
    "react",
    "chat",
    "widget",
    "openai"
  ],
  "author": "SearchAF",
  "license": "MIT"
}
